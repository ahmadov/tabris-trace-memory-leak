<?xml version='1.0' encoding='utf-8'?>
<widget id="ahmadov.tabristracememoryleak" version="0.1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>Tabris.js Trace Memory Leak</name>
  <description>
    Example Tabris.js App
  </description>
  <author email="eahmadov@eclipsesource.com">
    Elmi Ahmadov
  </author>
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />

  <platform name="android">

    <edit-config file="app/src/main/AndroidManifest.xml" target="/manifest/application" mode="merge">
      <application android:usesCleartextTraffic="true" />
    </edit-config>

  </platform>
</widget>
